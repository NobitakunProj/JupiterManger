<!doctype html>
<html lang="jp">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>JupiterManger</title>
</head>
<body>
<div id="contents">
  <%= render 'Templates/base' %>
  <div id="inner-contents">
    <div class="per-title">
      # Staff Master
      <a href="/">>HOME</a>
    </div>
    <!--    スタッフマスタビューモード -->
    <div id="staff-box">
      <div id="staff-head">
        <div class="staff-td">ID</div>
        <div class="staff-td">スタッフ名</div>
        <div class="staff-td">部署</div>
        <div class="staff-td">課</div>
        <div class="staff-td">チーム</div>
      </div>
      <div id="staff-head" class="js-staff-data">
        <div class="staff-td staff-id">
          <input type="text" name="staff-id" value="1" id="" class="short-box non-box">
        </div>
        <div class="staff-td">
          <input type="text" name="staff-name" value="マサラタウンサトシ" id="" class="short-box non-box">
        </div>
        <div class="staff-td">
          <input type="text" name="staff-dept1" value="マサラタウン" id="" class="short-box non-box">
        </div>
        <div class="staff-td">
          <input type="text" name="staff-dept2" value="オーキド課" id="" class="short-box non-box">
        </div>
        <div class="staff-td">
          <input type="text" name="staff-dept3" value="ポケモン図鑑チーム" id="" class="short-box non-box">
        </div>
      </div>
    </div>
    <!--    スタッフマスタビューモード -->
  </div>
</div>
</body>
</html>